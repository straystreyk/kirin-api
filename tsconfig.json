{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": [
      "es6",
    ],
    "noImplicitThis": true,
    "strictNullChecks": true,
    "resolveJsonModule": true
  }
}